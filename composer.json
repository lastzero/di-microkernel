{
    "name": "lastzero/di-microkernel",
    "description": "A micro-kernel for bootstrapping almost any PHP application",
    "type": "library",
    "keywords": [
        "silex",
        "symfony",
        "lumen",
        "dependency injection",
        "performance",
        "micro-framework"
    ],
    "license": "MIT",
    "homepage": "https://github.com/lastzero/di-microkernel",
    "authors": [
        {
            "name": "Michael Mayer",
            "email": "michael@lastzero.net",
            "homepage": "https://lastzero.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/dependency-injection": "^2.8|^3.0",
        "symfony/yaml": "^2.8|^3.0",
        "psr/container": "*"
    },
    "require-dev": {
        "lastzero/test-tools": "^3.0"
    },
    "autoload": {
        "psr-4": { "DIMicroKernel\\": "src/" }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
